import Part
from FreeCAD import Base
from FreeCAD import Gui
import math
import NURBSlib_EVM as Nl


s=Gui.Selection.getSelection()

if (len(s)!=4):
	print 'select four bezoid curves that form a loop counterclockwise'

c1=s[0].Shape.Curve
c2=s[1].Shape.Curve
c3=s[2].Shape.Curve
c4=s[3].Shape.Curve


quad66_patch=Nl.grid_66_quad(c1,c2,c3,c4)
patch66=Nl.BezCubic66_patch(quad66_patch)

Part.show(patch66.toShape())















